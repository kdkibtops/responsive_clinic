    "bugs": {
        "url": "https://github.com/kdkibtops/responsive_clinic/issues"
    },
    "homepage": "https://github.com/kdkibtops/responsive_clinic#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.12",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.25",
        "@types/pg": "^8.6.5",
        "ts-node": "^10.7.0",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "body-parser": "^1.20.0",
